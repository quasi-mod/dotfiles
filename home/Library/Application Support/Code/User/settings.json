{
    // Telemetry
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,

    "editor.tabSize": 4,
    "editor.detectIndentation": true,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.renderIndentGuides": true,
    "editor.fontSize":16,
    "editor.renderWhitespace": "boundary",
    "editor.lineNumbers": "relative",
    "editor.suggestSelection": "first",
    "terminal.integrated.fontSize": 16,
    "workbench.colorTheme": "Monokai Pro (Filter Spectrum)",
    "editor.wordWrap": "on",
    "editor.renderControlCharacters": true, // 制御文字を表示する
    "editor.renderLineHighlight": "all", // カーソル行を強調する
    "editor.cursorBlinking": "phase", // カーソルを点滅させる
    "editor.cursorSurroundingLines": 9, // スクロールを開始する行数

    "workbench.activityBar.visible": false, // 一番左のメニューバーを非表示
    "workbench.editor.enablePreview": false, // プレビュー機能をオフにする

    "window.restoreFullscreen": true, // 全画面を保持

    "search.exclude": { // 検索対象外のファイル
        "**/tags": true,
    },

    "files.trimTrailingWhitespace": true,
    "[cpp]": {
        "editor.quickSuggestions": true
    },
    "[c]": {
        "editor.quickSuggestions": true
    },
    "window.zoomLevel": 1,

    // Vim Related
    "vim.leader": "<space>",
    "vim.camelCaseMotion.enable": true,
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.visualstar": true,
    "vim.statusBarColorControl": true,
    "vim.useCtrlKeys": true,
    "vim.statusBarColors.normal": "#F16C2A",
    "vim.statusBarColors.insert": "#0758DE",
    "vim.statusBarColors.visual": "#92D67D",
    "vim.statusBarColors.visualline": "#92D67D",
    "vim.statusBarColors.visualblock": "#92D67D",
    "vim.statusBarColors.replace": "#B4435",

    "vim.vimrc.enable": true,
    "vim.vimrc.path": "~/.vimrc",

    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "rgb(230, 127, 110,0.7)",
    "workbench.colorCustomizations": {
        "statusBar.background": "#F16C2A",
        "statusBar.noFolderBackground": "#F16C2A",
        "statusBar.debuggingBackground": "#F16C2A"
    },

    "workbench.tips.enabled": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.shell.linux": "/bin/zsh",
    "workbench.startupEditor": "none",
    "workbench.statusBar.feedback.visible": false,
    "workbench.list.keyboardNavigation": "simple",
    "macros": {
        "focusFiles": [
            "workbench.action.focusSideBar",
            "outline.focus"
        ]
    },
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "latex-workshop.latex.external.build.command": "latexmk",
    "python.jediEnabled": true,
    "terminal.integrated.fontFamily": "Meslo LG S DZ for Powerline",
    "cSpell.userWords": [
        "LLVM",
        "struct"
    ],
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.external.linuxExec": "GNOME terminal",
    "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
    "hediet.vscode-drawio.local-storage": "",
    "workbench.editor.showTabs": false,
    "files.autoGuessEncoding": true
}