{
    // Telemetry
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,

    "editor.tabSize": 4,
    "editor.detectIndentation": true,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.renderIndentGuides": true,
    "editor.fontSize":16,
    "editor.renderWhitespace": "boundary",
    "editor.lineNumbers": "relative",
    "editor.suggestSelection": "first",
    "terminal.integrated.fontSize": 16,
    "workbench.colorTheme": "Monokai",
    "editor.wordWrap": "on",
    "files.trimTrailingWhitespace": true,
    "[cpp]": {
        "editor.quickSuggestions": true
    },
    "[c]": {
        "editor.quickSuggestions": true
    },
    "window.zoomLevel": 0,
    // Vim Related
    "vim.leader": "<space>",
    "vim.camelCaseMotion.enable": true,
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.visualstar": true,
    "vim.statusBarColorControl": true,
    "vim.useCtrlKeys": true,
    "vim.statusBarColors.normal": "#F16C2A",
    "vim.statusBarColors.insert": "#0758DE",
    "vim.statusBarColors.visual": "#92D67D",
    "vim.statusBarColors.visualline": "#92D67D",
    "vim.statusBarColors.visualblock": "#92D67D",
    "vim.statusBarColors.replace": "#B4435",
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "~/.vimrc",
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "rgb(230, 127, 110,0.7)",
    "workbench.colorCustomizations": {
        "statusBar.background": "#F16C2A",
        "statusBar.noFolderBackground": "#F16C2A",
        "statusBar.debuggingBackground": "#F16C2A"
    },
    "workbench.tips.enabled": false,
    "terminal.explorerKind": "external",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.shell.linux": "/bin/zsh",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.iconTheme": "vscode-icons",
    "workbench.statusBar.feedback.visible": false,
    "workbench.list.keyboardNavigation": "simple",
    "macros": {
        "focusFiles": [
            "workbench.action.focusSideBar",
            "outline.focus"
        ]
    },
    "sync.gist": "f1e8a2b78c37dc3bc8166ab95c140971",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "latex-workshop.latex.external.build.command": "latexmk",
    "python.jediEnabled": false
}
